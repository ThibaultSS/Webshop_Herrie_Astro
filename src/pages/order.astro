---
import Layout from '../layouts/BasicLayout.astro';
import ShoppingCart from '../components/ShoppingCart.astro';
import { API_URL } from "../config.js";
const pageTitle = "Shopping Cart";
const response = await fetch(`${API_URL}astro/get/shoppingcart`);
const carts = await response.json();
---

<Layout pageTitle={pageTitle}>
  <ShoppingCart carts={carts} />
</Layout>
